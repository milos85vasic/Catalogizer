        {
          "id": "SEC_001",
          "name": "SQL Injection Prevention",
          "description": "Test API endpoints against SQL injection attacks",
          "endpoint": "/api/v1/media/search",
          "method": "GET",
          "test_data": {
            "malicious_payloads": [
              "'; DROP TABLE files; --",
              "1' OR '1'='1",
              "UNION SELECT * FROM users",
              "'; UPDATE users SET password='hacked' WHERE id=1; --"
            ]
          },
          "expected_response": {
            "status": 400,
            "input_sanitized": true,
            "no_db_modification": true
          },
          "actual_result": {
            "status": 400,
            "payloads_blocked": 4,
            "database_integrity": "maintained",
            "logs_generated": true
          },
          "status": "PASSED"
        },
        {
          "id": "SEC_002",
          "name": "XSS Attack Prevention",
          "description": "Test XSS prevention in API responses",
          "endpoint": "/api/v1/media/metadata",
          "method": "POST",
          "test_data": {
            "xss_payloads": [
              "<script>alert('XSS')</script>",
              "javascript:alert('XSS')",
              "<img src=x onerror=alert('XSS')>",
              "';alert(String.fromCharCode(88,83,83))//'"
            ]
          },
          "expected_response": {
            "status": 200,
            "input_escaped": true,
            "no_script_execution": true
          },
          "actual_result": {
            "status": 200,
            "payloads_sanitized": 4,
            "output_escaped": true,
            "csp_headers_present": true
          },
          "status": "PASSED"
        }
      ]
